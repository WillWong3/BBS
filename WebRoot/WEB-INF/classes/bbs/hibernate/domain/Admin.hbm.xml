<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="bbs.hibernate.domain">
    <class name="Admin" table="admin">
        <id name="adminId">
            <generator class="native" />
        </id>
        <property name="adminName" type="java.lang.String" length="10" not-null="true"/>
        <property name="adminPassword" type="java.lang.String" length="16" not-null="true"/>
    </class>
</hibernate-mapping>
